    "dayjs": "^1.11.8",
    "echarts": "^5.4.2",
    "element-plus": "^2.3.6",
    "js-cookie": "^3.0.5",
    "jsencrypt": "^3.3.2",
    "mitt": "^3.0.0",
    "mint-filter": "^4.0.3",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "path": "^0.12.7",
    "pinia": "^2.1.4",
    "pinyin-pro": "^3.15.2",
