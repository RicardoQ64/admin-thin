  "npm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "rollup",
        "webpack",
        "core-js"
      ]
    },
    "allowedDeprecatedVersions": {
      "sourcemap-codec": "*",
      "w3c-hr-time": "*",
      "stable": "*"
    }
  }
